
generator client {
  provider = "prisma-client-js"
}


datasource db {
  provider = "mongodb"
  url      = env("MONGO_URI")
}

model User {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  email     String   @unique
  username  String   @unique
  password  String
  avatar    String?
  createdAt DateTime @default(now())
}